		ИдСправкиБ = ?(ДопПараметрыДляЕГАИС <> Неопределено И ДопПараметрыДляЕГАИС.Свойство("ИдСправкиБ"), ДопПараметрыДляЕГАИС.ИдСправкиБ, "");
			
		Запрос = Новый Запрос;
